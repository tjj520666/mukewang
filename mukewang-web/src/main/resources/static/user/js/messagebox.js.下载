define(function(){function c(s,m,t){var c,g=a[s];g&&(c=$(g),c.find(".mmsg-content p").text(m),c.appendTo("body"),c.css({marginTop:c.height()/-2,marginLeft:Math.min(600,c.width())/-2}).fadeIn().delay(t||2e3).fadeOut(function(){}))}var a={info:'<div class="mmsg-box mmsg-box-info"><div class="mmsg-content"><i class="mmsg-icon"></i><p></p></div><div class="mmsg-background"></div></div>',error:'<div class="mmsg-box mmsg-box-error"><div class="mmsg-content"><i class="mmsg-icon"></i><p></p></div><div class="mmsg-background"></div></div>'};return{info:function(m,t){c("info",m,t)},error:function(m,t){c("error",m,t)}}});